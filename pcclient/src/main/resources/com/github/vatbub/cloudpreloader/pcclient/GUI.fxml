<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="116.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.vatbub.cloudpreloader.pcclient.GUI">
   <children>
      <GridPane hgap="5.0" vgap="5.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="enterUrl" maxWidth="1.7976931348623157E308" promptText="%textbox.enterurl.prompttext" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" />
            <Label text="%label.enterurltitle.text" GridPane.columnSpan="2147483647" />
            <ChoiceBox fx:id="servicePicker" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <VBox alignment="CENTER" maxWidth="1.7976931348623157E308" GridPane.columnIndex="2" GridPane.rowIndex="1">
               <children>
                  <Button fx:id="send" defaultButton="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#sendOnAction" text="%button.send.text" VBox.vgrow="ALWAYS" />
                  <Button fx:id="setup" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#setupOnAction" text="%button.setup.text" GridPane.columnIndex="2" GridPane.rowIndex="1" />
               </children>
            </VBox>
            <CheckBox fx:id="allowInBackground" mnemonicParsing="false" onAction="#allowInBackgroundOnAction" text="%checkbox.allowinbackground.text" GridPane.columnSpan="2147483647" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
